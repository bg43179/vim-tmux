# quick directory movement
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias cdd="cd ~/Desktop"
alias ws="cd ~/workspace"

alias vim='printf "\033[2 q"; mvim -v'
#alias vim='mvim -v'
alias vimrc="vim ~/.vimrc"
alias zshrc="vim ~/.zshrc"
alias ipython='printf "\033[2 q"; ipython --TerminalInteractiveShell.editing_mode=vi'
alias hisnu="history | awk '{$1="";print substr($0,2)}'"
